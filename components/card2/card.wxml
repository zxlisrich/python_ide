<navigator wx:if="{{elements.title != null}}" open-type="navigate" hover-class='none' bindtap="goto_next" data-index="{{elements.index}}"
  class=" nav-li bc" style="width:300rpx; padding-right:100rpx; height:200rpx" >
  <view class="nav-title">{{elements.title}}</view>
  <text wx:if="{{elements.ispass == 0}}" class='cuIcon-attentionforbid'></text>
  <text wx:elif="{{elements.ispass == 1}}" class='cuIcon-check'></text>
  <text wx:else class='cuIcon-attention'></text>
</navigator>